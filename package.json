
{
  "name": "radio-iqra-bf-render",
  "version": "1.1.0",
  "description": "Radio Live with Prayer Times - Fixed for Render",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "build": "mkdir -p dist && esbuild index.tsx --bundle --outfile=dist/bundle.js --loader:.tsx=tsx --loader:.ts=ts --minify --sourcemap --platform=browser --format=iife --define:process.env.NODE_ENV='\"production\"' --define:process.env.API_KEY='\"'\"$API_KEY\"'\"'",
    "dev": "nodemon server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.0.3",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "@google/genai": "^1.34.0"
  },
  "devDependencies": {
    "esbuild": "^0.20.0",
    "nodemon": "^2.0.22"
  }
}
